prefix=/usr
includedir=${prefix}/include
libdir=${prefix}/lib

Name: Pango Xft
Description: Xft font support for Pango
Version: 1.50.11
Requires: pangoft2, xft >=  2.0.0
Requires.private: pango, glib-2.0 >=  2.62, gobject-2.0 >=  2.62, gio-2.0 >=  2.62, fribidi >=  1.0.6, libthai >=  0.1.9, harfbuzz >=  2.6.0, fontconfig >=  2.13.0, freetype2, xrender, cairo >=  1.12.10, harfbuzz-gobject >=  2.6.0
Libs: -L${libdir} -lpangoxft-1.0
Libs.private: -lm
Cflags: -I${includedir}/pango-1.0
